{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "webi#dev": {
      "env": ["DATABASE_URL"]
    },
    "location-api#dev": {
      "env": ["DATABASE_URL", "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY"]
    },
    "build": {
      "outputs": [".next/**", "dist/**", "storybook-static/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
